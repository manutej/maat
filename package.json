{
  "name": "@luxor/observe",
  "version": "2.0.0",
  "description": "Pure functional observability with categorical foundations",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "observe": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "observe": "ts-node index.ts"
  },
  "keywords": [
    "observability",
    "category-theory",
    "functional-programming",
    "comonad",
    "fp-ts"
  ],
  "author": "LUXOR",
  "license": "MIT",
  "dependencies": {
    "fp-ts": "^2.16.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
